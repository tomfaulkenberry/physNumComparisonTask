define sequence experiment
	set flush_keyboard yes
	set description "Runs a number of items in sequence"
	run instruction1 always
	run instruction2 always
	run experimentalBlock1 always
	run break1 always
	run experimentalBlock2 always
	run break2 always
	run experimentalBlock3 always
	run break3 always
	run experimentalBlock4 always
	run break4 always

